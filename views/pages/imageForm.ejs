    <title>Fileupload</title>
</head>

<body>
    <h1>Fileupload</h1>
    <form action="/image" method="post" enctype="multipart/form-data">
        <div class="inputGroup">
            <label for="upload__image">File</label>
            <input type="file" name="billede" id="upload__image">
